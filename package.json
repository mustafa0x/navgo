{
	"name": "navgo",
	"version": "3.0.6",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mustafa0x/navgo.git"
	},
	"description": "Adavanced router",
	"module": "./index.js",
	"type": "module",
	"exports": "./index.js",
	"types": "index.d.ts",
	"license": "MIT",
	"author": {
		"name": "mustafa j"
	},
	"scripts": {
		"build": "perl -0777 -i -pe 's/console.debug\\(...args\\)/{}/g' index.js",
		"prepublishOnly": "pnpm test && pnpm test:e2e && pnpm run build",
		"test": "vitest run index.test.js",
		"test:e2e": "playwright test",
		"start:testsite": "pnpm vite dev test/site --port 5714",
		"types": "tsc -p test/types"
	},
	"files": [
		"*.d.ts",
		"index.js"
	],
	"keywords": [
		"browser",
		"client-side",
		"router"
	],
	"dependencies": {
		"regexparam": "^3.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.37.0",
		"@playwright/test": "^1.56.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@tsconfig/svelte": "^5.0.5",
		"@types/node": "^24.7.2",
		"eslint": "^9.37.0",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"jiti": "^2.6.1",
		"lightningcss": "^1.30.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.11",
		"tailwindcss": "^4.1.14",
		"terser": "5.44.0",
		"typescript": "5.9.3",
		"typescript-eslint": "^8.46.0",
		"vite": "^7.1.9",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"printWidth": 100,
		"quoteProps": "as-needed",
		"bracketSpacing": true,
		"arrowParens": "avoid",
		"useTabs": true,
		"overrides": [
			{
				"files": "*.md",
				"options": {
					"tabWidth": 2,
					"useTabs": false
				}
			}
		]
	}
}
